generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

// start trello-clone schema

model Board {
  id        String   @id @default(cuid())
  title     String   @db.Text
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

// end trello-clone schema
